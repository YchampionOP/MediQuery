# Environment Configuration
NODE_ENV=development

# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:3000

# Elasticsearch Configuration
ELASTICSEARCH_NODE=https://your-cluster.es.europe-west3.gcp.cloud.es.io
ELASTICSEARCH_API_KEY=your_api_key_here
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=your_password_here

# Medical API Keys (for future integrations)
MEDICAL_API_KEY=your_medical_api_key
DRUG_INTERACTION_API_KEY=your_drug_api_key

# Local LLM Configuration
LLM_MODEL_PATH=./ai-models/medical-llm
EMBEDDINGS_MODEL_PATH=./ai-models/medical-embeddings

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Data Processing
MIMIC_DATA_PATH=./mimic-iii-clinical-database-demo-1.4
SYNTHEA_DATA_PATH=./synthea-master/output
BATCH_SIZE=1000

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/mediquery.log

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100